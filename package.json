{
  "name": "relief-app",
  "version": "1.0.0",
  "description": "Relief App API",
  "author": "Phi Thuong Nguyen",
  "private": true,
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "generated:format": "prettier --write \"src/generated/**/*.ts\"",
    "dev": "nest start --debug --watch",
    "start:dev": "nest start --debug --watch",
    "start:prod": "node dist/src/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest --colors --verbose --detectOpenHandles",
    "test:unit": "yarn test --config ./jest.config-unit.js",
    "test:e2e": "yarn test --config ./jest.config-e2e.js",
    "test:ci": "yarn test --ci --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "detect-secrets": "git diff --staged --name-only | xargs detect-secrets-hook --baseline .secrets.baseline",
    "detect-secrets:scan": "detect-secrets scan --baseline .secrets.baseline",
    "detect-secrets:audit": "detect-secrets audit .secrets.baseline",
    "prisma:m": "npx prisma migrate dev",
    "prisma:m-c": "npx prisma migrate dev --create-only",
    "prisma:g": "npx prisma generate",
    "prisma:i": "npx prisma introspect",
    "prisma:deploy": "npx prisma migrate deploy",
    "commit": "git-cz",
    "prepare": "husky install"
  },
  "dependencies": {
    "@nestjs/common": "^8.4.3",
    "@nestjs/config": "^1.1.6",
    "@nestjs/core": "^8.2.6",
    "@nestjs/cqrs": "^8.0.1",
    "@nestjs/jwt": "^10.0.3",
    "@nestjs/passport": "^8.1.0",
    "@nestjs/platform-express": "^8.2.5",
    "@nestjs/swagger": "^6.3.0",
    "@nestjs/testing": "^8.2.5",
    "@prisma/client": "^4.11.0",
    "aws-sdk": "^2.1072.0",
    "bcryptjs": "^2.4.3",
    "cache-manager": "^3.6.1",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.13.2",
    "compression": "^1.7.4",
    "dayjs": "^1.11.7",
    "decimal.js": "^10.4.1",
    "express": "^4.17.2",
    "generate-password": "^1.7.0",
    "helmet": "^5.0.1",
    "jest-junit": "^13.0.0",
    "joi": "^17.5.0",
    "jwks-rsa": "^2.0.5",
    "lodash": "^4.17.21",
    "passport": "^0.5.2",
    "passport-jwt": "^4.0.0",
    "passport-local": "^1.0.0",
    "rand-token": "^1.0.1",
    "reflect-metadata": "^0.1.13",
    "sqs-consumer": "^5.4.0",
    "sqs-producer": "^2.1.0",
    "swagger-ui-express": "^4.3.0"
  },
  "resolutions": {
    "minimist": "^1.2.6",
    "node-fetch": "2.6.7"
  },
  "devDependencies": {
    "@brakebein/prisma-generator-nestjs-dto": "^1.13.0",
    "@commitlint/cli": "^16.0.2",
    "@commitlint/config-conventional": "^16.0.0",
    "@types/bcryptjs": "^2.4.2",
    "@types/cache-manager": "^3.4.3",
    "@types/compression": "^1.7.2",
    "@types/helmet": "^4.0.0",
    "@types/jest": "27.0.2",
    "@types/lodash": "^4.14.178",
    "@types/node": "^16.0.0",
    "@types/node-fetch": "^2.6.2",
    "@types/passport-jwt": "^3.0.6",
    "@types/passport-local": "^1.0.0",
    "@types/supertest": "^2.0.12",
    "@typescript-eslint/eslint-plugin": "^5.0.0",
    "@typescript-eslint/parser": "^5.0.0",
    "cz-conventional-changelog": "^3.3.0",
    "eslint": "^8.0.1",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-import": "^2.25.4",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-security": "^1.4.0",
    "husky": "^7.0.0",
    "jest": "^27.5.1",
    "nestjs-pino": "^3.1.3",
    "prettier": "^2.3.2",
    "prisma": "^4.11.0",
    "source-map-support": "^0.5.20",
    "supertest": "^6.1.3",
    "ts-jest": "^27.0.3",
    "ts-loader": "^9.2.3",
    "ts-mockito": "^2.6.1",
    "ts-node": "^10.0.0",
    "tsconfig-paths": "^3.14.1",
    "typescript": "^4.3.5",
    "pino-pretty": "^7.3.0",
    "pino-http": "^6.5.0"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
